Tab:AddButton({
	Name = "Jump Pred Fucker",
	Callback = function()
      		print("Jump Prediction Fucker Enabled")--- Prints Console Command
            ---
              OrionLib:MakeNotification({
                Name = "Keybinds",
                Content = "No Keybinds!",--- Notifying User On Keybinds
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            ---
            local Plr = game.Players.LocalPlayer

            Plr.Character:WaitForChild("Humanoid").StateChanged:Connect(function(old,new)
                if new == Enum.HumanoidStateType.Freefall == true then
                    wait(0.27)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -15, 0)
                end
            end)
            if not game.IsLoaded(game) then 
                game.Loaded.Wait(game.Loaded);
            end
            
            -- variables 
            local IsA = game.IsA;
            local newindex = nil 
            
            -- main hook
            newindex = hookmetamethod(game, "__newindex", function(self, Index, Value)
                if not checkcaller() and IsA(self, "Humanoid") and Index == "JumpPower" then 
                    return
                end
                
                return newindex(self, Index, Value);
            end)
  	end    
})

